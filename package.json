{
    "name": "@roman86/vuejs-sdk",
    "version": "0.1.0",
    "description": "vuejs sdk",
    "main": "dist/vuejs-sdk.common.js",
    "scripts": {
        "build": "vue-cli-service build --target lib --name vuejs-sdk ./src/install.js"
    },
    "files": [
        "dist/*",
        "README.md"
    ],
    "license": "MIT",
    "dependencies": {
        "core-js": "^3.8.3",
        "vue": "^3.2.13"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "@vue/cli-plugin-babel": "~5.0.0",
        "@vue/cli-plugin-eslint": "~5.0.0",
        "@vue/cli-service": "~5.0.0",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^8.0.3"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "@babel/eslint-parser"
        },
        "rules": {}
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ]
}
